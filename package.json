{
  "name": "directuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bash ./scripts/dev.sh",
    "prod": "bash ./scripts/prod.sh",
    "backup": "bash ./scripts/backup.sh backup",
    "restore": "bash ./scripts/restore.sh restore",
    "export-template": "bash ./scripts/export-template.sh",
