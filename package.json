    "lint": "pnpm --filter nuxt run lint",
    "lintfix": "pnpm --filter nuxt run lintfix"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=20.14.0",
    "pnpm": ">=9"
  }
}
